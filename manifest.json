{
  "description": "Blocks and unwraps Google tracking requests",
  "manifest_version": 2,
  "name": "GoogleRedirectUnwrap",
  "version": "1.0",

  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "https://www.google.com/"
  ],

  "background": {
    "scripts": ["background.js"]
  }
}
